    "def quadratic_loss(y, hx):\n",
    "    return (y - hx)**2\n",
    "    \n",
    "def error(h, X, y):\n",
    "    err = 0\n",
    "    for xi, yi in zip(X, y):\n",
    "        err += quadratic_loss(yi, h(xi))\n",
    "    return 0.5 * err"
   ]
