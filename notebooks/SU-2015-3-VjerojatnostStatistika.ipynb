    "  plt.vlines(x, 0, p.pmf(x), colors='b', lw=5, alpha=0.5)\n",
    "plt.xlim(xmin=-1, xmax=2)\n",
    "plt.ylim(ymax=1)\n",
    "plt.show()"
   ]
